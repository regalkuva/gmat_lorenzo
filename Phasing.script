GMAT TADiff.RowTextColors = {2, [0 0 0]};
GMAT TADiff.RowTextColors = {3, [0 0 0]};
GMAT TADiff.RowTextColors = {4, [0 0 0]};
GMAT TADiff.WarnColor = [218 165 32];
GMAT TADiff.CritColor = [255 0 0];

%----------------------------------------
%---------- Arrays, Variables, Strings
%----------------------------------------
Create Variable TA_Lower TA_Higher TA_Difference DistanceBtwSC;
GMAT TA_Lower = 0;
GMAT TA_Higher = 0;
GMAT TA_Difference = 0;
GMAT DistanceBtwSC = 0;






%----------------------------------------
%---------- Mission Sequence
%----------------------------------------

BeginMissionSequence;
Propagate DefaultProp(DefaultLower, DefaultHigher) {DefaultHigher.ElapsedDays = 2};
Target DefaultDC {SolveMode = Solve, ExitMode = SaveAndContinue, ShowProgressWindow = true};
   Vary DefaultDC(ImpulsiveBurn1.Element3 = 0.001, {Perturbation = 0.0001, Lower = 0.0, Upper = 10, MaxStep = 0.5, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
   Propagate DefaultProp(DefaultHigher, DefaultLower) {DefaultHigher.Earth.Periapsis};
   Maneuver ImpulsiveBurn1(DefaultHigher);
   Achieve DefaultDC(DefaultHigher.EarthMJ2000Eq.BrouwerShortSMA = 7791.938817629014, {Tolerance = 0.1});
EndTarget;  % For targeter DefaultDC
Target DefaultDC {SolveMode = Solve, ExitMode = DiscardAndContinue, ShowProgressWindow = true};
   Vary DefaultDC(ImpulsiveBurn1.Element3 = 0.5, {Perturbation = 0.0001, Lower = 0.0, Upper = 3.14159, MaxStep = 0.2, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
   Propagate DefaultProp(DefaultLower, DefaultHigher) {DefaultHigher.Earth.Apoapsis};
   Maneuver ImpulsiveBurn1(DefaultHigher);
   Achieve DefaultDC(DefaultHigher.EarthMJ2000Eq.BrouwerShortECC = DefaultLower.EarthMJ2000Eq.BrouwerShortECC, {Tolerance = 0.0001});
EndTarget;  % For targeter DefaultDC
Propagate DefaultProp(DefaultLower, DefaultHigher) {DefaultHigher.ElapsedDays = 1};
Propagate DefaultProp(DefaultLower, DefaultHigher) {DefaultHigher.Earth.Apoapsis};
Target DefaultDC {SolveMode = Solve, ExitMode = DiscardAndContinue, ShowProgressWindow = true};
   Vary DefaultDC(ImpulsiveBurn2.Element3 = 0.001, {Perturbation = 0.0001, Lower = 0.0, Upper = 3.14159, MaxStep = 0.2, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1});
   Maneuver ImpulsiveBurn2(DefaultHigher);
   Achieve DefaultDC(DefaultHigher.Earth.RadPer = DefaultLower.Earth.RadPer, {Tolerance = 0.000001});
EndTarget;  % For targeter DefaultDC
Target DefaultDC {SolveMode = Solve, ExitMode = DiscardAndContinue, ShowProgressWindow = true};
   Vary DefaultDC(ImpulsiveBurn2.Element3 = 0.5, {Perturbation = 0.0001, Lower = 0.0, Upper = 3.14159, MaxStep = 0.2, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
   Propagate DefaultProp(DefaultLower, DefaultHigher) {DefaultHigher.Earth.Periapsis};
   Maneuver ImpulsiveBurn2(DefaultHigher);
   Achieve DefaultDC(DefaultHigher.Earth.RadApo = DefaultLower.Earth.RadApo, {Tolerance = 0.00001});
EndTarget;  % For targeter DefaultDC
Propagate DefaultProp(DefaultLower, DefaultHigher) {DefaultHigher.ElapsedDays = 3};

BeginScript
   GMAT TA_Lower = DefaultLower.TA;
   GMAT TA_Higher = DefaultHigher.TA;
   GMAT TA_Difference = abs(TA_Higher-TA_Lower);
   GMAT DistanceBtwSC = abs(DefaultHigher.Earth.RMAG - DefaultLower.Earth.RMAG);
EndScript;
UpdateDynamicData TADiff;
