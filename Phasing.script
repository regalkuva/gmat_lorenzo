Create DynamicDataDisplay TADiff;
GMAT TADiff.UpperLeft = [ 0.003418803418803419 0.7412060301507538 ];
GMAT TADiff.Size = [ 0.1264957264957265 0.3249581239530988 ];
GMAT TADiff.RelativeZOrder = 91;
GMAT TADiff.Maximized = false;
GMAT TADiff.AddParameters = {1, TA_Difference};
GMAT TADiff.AddParameters = {2, TA_Higher};
GMAT TADiff.AddParameters = {3, TA_Lower};
GMAT TADiff.AddParameters = {4, DistanceBtwSC};
